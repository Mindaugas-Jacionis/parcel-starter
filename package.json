{
  "name": "parcel-starter",
  "version": "2.0.0",
  "description": "Minimal starter setup with parcel for my students",
  "scripts": {
    "clean": "rm -rf public build dist",
    "prebuild": "npm run clean",
    "build": "parcel build ./src/index.html --out-dir build --public-url ./ --no-source-maps",
    "predev": "npm run clean",
    "develop": "parcel ./src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mindaugas-Jacionis/parcel-starter.git"
  },
  "keywords": [
    "parcel",
    "modularJS",
    "JavaScript"
  ],
